{
    "manifest_version": 3,
    "name": "Proxy Setter",
    "version": "1.0",
    "description": "代理配置",
    "permissions": [
        "proxy",
        "webRequest",
        "webRequestAuthProvider",
        "storage",
        "browsingData"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "worker.js"
    },
    "icons": {
        "48": "icon48.png",
        "128": "icon128.png"
    },
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    }
}